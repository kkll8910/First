.NavigateTabContainer_container{
    width:100%;
    height:100%
}

.NavigateTabContainer_tablist{
   width:100%;
   height:36px;
   #padding-left:80px;
   background-color:#eee;
}

.NavigateTabContainer_contentlist{
   width:100%;
   overflow:hidden;
   border-left:solid #eee 1px;
   padding:10px;
   
}


.NavigateTabContainer_iframe{
    width:100%;
    height:100%;	
    display:none;
    border:0px;
    overflow:auto;
    margin:0px;
}

.NavigateTabContainer_contentContainer{
	  width:100%;
    height:98%;	
    display:none;
    overflow:auto;
}

.NavigateTabContainer_tab{
	 float:left;
	 font-size:14px;
	 color:#000000;
	 padding:6px;
	 height:23px;
	 line-height:24px;
	 background-color:transparent;
	 cursor:pointer;
	 text-align:center;
}

.NavigateTabContainer_tab_selected{
	 border:solid #5c6670 1px;
	 border-bottom:0;
	 font-size:14px;
	 color: #069;
	 
	 background-color:#FFF;
	 padding:6px;
	 
	 -moz-border-radius: 6px 6px 0px 0px;      /* Gecko browsers */
   -webkit-border-radius: 6px 6px 0px 0px;   /* Webkit browsers */
   border-radius: 6px 6px 0px 0px;            /* W3C syntax */
}

.NavigateTabContainer_tab_titletext{
	min-width:60px;
	text-align:center;
	margin:0 4px 0 4px;
	float:left;
	text-shadow:none;
}

.NavigateTabContainer_closebutton{
	float:right;
	width:12px;
}