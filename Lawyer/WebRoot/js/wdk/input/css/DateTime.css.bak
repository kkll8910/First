.datetime_inputContainer{
	  clear:both;
	  float:left;
	  height: 34px;
	  line-height: 34px;
	  
	  border: 1px solid #ccc;
	  
	  border-radius: 4px;
	  -moz-border-radius: 4px;
    -webkit-border-radius:4px;	  
}

.datetime_title{
	  float:left;
    height: 34px;
	  line-height: 34px;
}

.inputContainer input{
	  float:left;
    height: 34px;
	  line-height: 34px;
	  border: 0px;	
	  text-align:right;
	  
	  display: block;
	  padding: 6px 12px;
	  font-size: 14px;
	  line-height: 1.42857143;
	  color:#555;
	  background-color: #fff;
}

.datatime_unit{
	  float:left;
	  height: 34px;
	  line-height: 34px;
	  margin:0px 10px 0px 10px;
}