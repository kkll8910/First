.Accordion_container{
	  width:100%;
	  float:left;
}

.Accordion_titleBar{
	float:left;
	background-color:#284452;
	width:100%;
	border:solid #284452 1px;
	border-bottom:solid #6a6a6a 1px;
	cursor:pointer;
}

.Accordion_icon{
	 float:left;
	 height:15px;
	 width:15px;
	 margin-left:7px 10px 0px 10px;
}

.Accordion_title{
	 float:left;
	 color:#FFFFFF;
	 height:30px;
	 line-height:30px;
	 font-weight:bold;
	 font-size:14px;
}

.Accordion_buttonList{
	 float:right;
	 
}

.Accordion_button{
	  float:right;
	  margin:5px 5px 0 0;
	  font-size:12px;
	  color:#FF6600;
}

.Accordion_button:hover{
	  color:#ddd;
}

.Accordion_contentList{
	  float:left;
	  width:100%;
	  background-color:#32505f;
	  border:solid #32505f 1px;
	  border-top:0px;
	  display:none;
}
     
.Accordion_content_ul{     
    list-style-type:none;
    margin:10px 0 0 10px;
    padding-left:10px;
    
}

.Accordion_content_li{     
    list-style-type:none;
    color:#FFFFFF;
    margin:10px;
}