.Dialog_container{
	 position:absolute;
	 
   -webkit-box-shadow: #666 0px 0px 10px;
   -moz-box-shadow: #666 0px 0px 10px;
   box-shadow: #666 0px 0px 10px;
   
   -moz-border-radius: 6px 6px 0px 0px;      /* Gecko browsers */
   -webkit-border-radius: 6px 6px 0px 0px;   /* Webkit browsers */
   border-radius: 6px 6px 0px 0px;            /* W3C syntax */
   
   border:solid gray 1px;
   background-color:#ddd;
   display:none;
}

.Dialog_background{
	  display:none;
	  position:absolute;
	  left:0px;
	  top:0px;
	  width:100%;
	  height:100%;
	  background-color:#ddd;
	  
	  filter:alpha(opacity=50);  
    -moz-opacity:0.5;  
    -khtml-opacity: 0.5;  
    opacity: 0.5;  
}

.Dialog_titleBar{
	 height:40px;
	 background-color:#32505f;
   -moz-border-radius: 6px 6px 0px 0px;      /* Gecko browsers */
   -webkit-border-radius: 6px 6px 0px 0px;   /* Webkit browsers */
   border-radius: 6px 6px 0px 0px;            /* W3C syntax */
}

.Dialog_title{
	  height:100%;
	  line-height:40px;
	  color:#FFFFFF;
	  margin-left:10px;
}

.Dialog_exitbutton{
	float:right;
	margin-right:5px;
	margin-top:10px;
}

.Dialog_content{
	 overflow:auto;
   color:#000000;
   margin:4px;
   width:100%;
}

.Dialog_toolbar{
   width:100%;
   margin-bottom:5px;
}

.Dialog_button{
	  float:right;
	
	  height:24px;
    line-height:24px;
    min-width:60px;
    
    -moz-border-radius: 2px;      
    -webkit-border-radius: 2px;   
    
    border-radius: 2px;  
    font-size:14px;
    vertical-align:middle;
    cursor:pointer;
    margin-right:10px;
    margin-bottom:5px;
    padding:3px 5px;
    text-align:center;
    -webkit-box-shadow: #666 0px 0px 5px;
    -moz-box-shadow: #666 0px 0px 5px;
    box-shadow: #666 0px 0px 5px;
}

.Dialog_button_hover{
	  background-color:#006699;
	  color:#FFFFFF;
}